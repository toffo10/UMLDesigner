<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="841.0" maxWidth="880.0" minHeight="400.0" minWidth="400.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="841.0" prefWidth="880.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ent.umldesigner.DrawingController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <SplitPane dividerPositions="0.5" VBox.vgrow="ALWAYS">
        <items>
            <TextArea fx:id="umlTextArea" />
            <Pane fx:id="drawingArea" />
        </items>
    </SplitPane>
    <TextArea fx:id="resultArea" editable="false" />
    <Button onAction="#convertUml" text="Genera UML">
        <font>
            <Font size="27.0" />
        </font>
    </Button>
</VBox>
